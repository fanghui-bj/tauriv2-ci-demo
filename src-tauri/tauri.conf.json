  "productName": "tauri-ci-demo",
  "version": "0.2.3",
  "identifier": "com.tauri-ci-demo.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
