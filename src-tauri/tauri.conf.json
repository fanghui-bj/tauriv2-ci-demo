{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-ci-demo",
  "version": "0.1.0",
  "identifier": "com.tauri-ci-demo.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-ci-demo",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDk0MDQ1MjgwRkVDMUI4RTUKUldUbHVNSCtnRklFbExYVUVNK1FRTDZkWU9JNUd0MFBTbVpoZjhXc3gwTkNuNmFDQ0dSRmNzUHQK",
      "endpoints": ["https://github.com/fanghui-bj/tauriv2-ci-demo/releases/latest/download/latest.json"],
      "dangerousInsecureTransportProtocol": true
    }
  }
}
